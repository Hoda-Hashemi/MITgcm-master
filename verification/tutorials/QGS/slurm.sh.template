#!/bin/bash
#SBATCH --job-name=qgs_init
#SBATCH --output=qgs.out
#SBATCH --error=qgs.err
#SBATCH --time=02:00:00
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=48
